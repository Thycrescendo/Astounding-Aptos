  "dependencies": {
    "aphrodite": "^2.4.0",
    "aptos": "^1.1.0",
    "bootstrap": "^5.3.3",
    "history": "^5.3.0",
    "node-sass-chokidar": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-circular-progressbar": "^2.1.0",
    "react-dom": "^18.3.1",
    "react-fontawesome": "^1.7.1",
    "react-motion": "^0.5.2",
    "react-redux": "^9.1.2",
    "react-router": "^6.23.1",
    "react-router-redux": "4.0.8",
    "react-scripts": "^5.0.1",
    "react-share": "^5.1.0",
    "react-svg": "^16.1.34",
    "react-switch": "^7.0.0",
    "react-tradingview-widget": "^1.3.2",
    "reactstrap": "^9.2.2",
    "recharts": "^2.12.7",
    "redux": "^5.0.1",
